      <RIB_INFO MODE="write">
          <BLOB_WRITE>
              <BLOB_NAMESPACE VALUE="tmp"/>
              <BLOB_NAME VALUE="foo"/>
              <BLOBSTORE_TYPE VALUE="text/plain"/>
              <BLOB_APPEND VALUE="y"/>
              <BLOB_FINAL VALUE="y"/>
              <BLOB_TOTAL_SIZE VALUE="9576"/>
              <BLOB_DATA>
                  -----BEGIN BLOB-----
                  ...
                  -----END BLOB-----
              </BLOB_DATA>
          </BLOB_WRITE>
      </RIB_INFO>
  </LOGIN>
</RIBCL>
