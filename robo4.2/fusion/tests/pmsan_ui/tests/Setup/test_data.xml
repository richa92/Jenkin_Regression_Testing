<data>
	<!-- The Environment to setup the system -->
	<environment_configuration>
		<env seleniumSpeed="0.1" applianceIP="https://siddvenk.vse.adapps.hp.com" browser="chrome"/>
	</environment_configuration>

	<!-- The User who will log on to the appliance -->
	<users>
		<user name="Administrator" password="hpvse123"/>
	</users>
	
	<!-- San Manager -->
	<sanmanagers>
		<sanmanager sanip="172.18.15.1" type="Brocade Network Advisor" username="dcs" password="dcs" sanmanagerssl="true"/>
	</sanmanagers>
	
	<!-- FC networks -->
	<fcnetwork1>
		<fcnetwork name="direct_network_1" fabrictype="DIRECT ATTACH"/>
	</fcnetwork1>
	<fcnetwork2>
		<fcnetwork name="direct_network_2" fabrictype="DIRECT ATTACH"/>
	</fcnetwork2>
	<fcnetwork3>
		<fcnetwork name="fabric_network_1" fabrictype="FABRIC ATTACH" san="SAN1_0"/>
	</fcnetwork3>
	<fcnetwork4>
		<fcnetwork name="fabric_network_2" fabrictype="FABRIC ATTACH" san="SAN1_1"/>
	</fcnetwork4>
	
	<!-- LIG -->
	<ligs>
		<lig name="LIG"
			fastmaccachefailover="true"
			macrefreshinterval="5"
			igmpsnooping="false"
			igmpidletimeout="260"
			loopprotection="true"
			pausefloodprotection="true"
			qos_configuration_type="Passthrough"
			internal_networks="">
			<switch bay="1" type="HP VC FlexFabric 10Gb/24-Port Module"/>
			<switch bay="2" type="HP VC FlexFabric 10Gb/24-Port Module"/>
			<lus connectionMode="AUTO" lacptimer="" name="link_1" native="" networkType="Fibre Channel" network="direct_network_1" ports="bay1:X1:Auto"/>
			<lus connectionMode="AUTO" lacptimer="" name="link_2" native="" networkType="Fibre Channel" network="direct_network_2" ports="bay2:X2:Auto"/>
			<lus connectionMode="AUTO" lacptimer="" name="link_3" native="" networkType="Fibre Channel" network="fabric_network_1" ports="bay1:X2:Auto"/>
			<lus connectionMode="AUTO" lacptimer="" name="link_4" native="" networkType="Fibre Channel" network="fabric_network_2" ports="bay2:X2:Auto"/>
		</lig>
	</ligs>
	
	<!-- Enclosure Group -->
	<enclgroups>
		<enclosuregroup name="group_1">
			<switch lig="LIG" bay="1"/>
			<switch lig="LIG" bay="2"/>
		</enclosuregroup>
	</enclgroups>
	
	<!-- Enclosure -->
	<enclosures>
		<enclosure name="enclosure_1"
				oa1hostname="172.18.1.11"
				oa1username="dcs"
				oa1password="dcs"
				create_new_eg="false"
				encgroup="group_1"
				force="false"
				addas="Managed"
				manageFirmware="false"
				licensing="HP OneView Advanced"
				frimwareBaseline="Manage manually"
				spp=""
				upDateFor="">
		</enclosure>
	</enclosures>
	
	<!-- Storage System -->
	<storagesystems>
		<storage name="172.18.11.12" userid="dcs" pswd="dcs" domain="TestDomain"/>
	</storagesystems>
	
	<!-- Storage Pools -->
	<pools>
		<pool storagename="ThreePAR7200-5612" pool="FST_CPG1"/>
		<pool storagename="ThreePAR7200-5612" pool="FST_CPG2"/>
	</pools>
	
</data>