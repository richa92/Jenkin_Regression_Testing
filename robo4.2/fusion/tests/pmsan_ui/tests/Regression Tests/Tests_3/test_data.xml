            </sanstorage>
            <sanstorage sanvolume="volume2" san="true" volumetype="New volume" sanlun="52" storagepool="FST_CPG2" ostype="Windows 2012 / WS2012 R2" capacity="4" provisioning="Full" permanent="False">
