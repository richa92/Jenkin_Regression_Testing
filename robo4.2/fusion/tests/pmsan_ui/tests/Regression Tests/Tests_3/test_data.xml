				<connection name="fabric_network_1">
					<port target="20:00:00:02:AC:00:08:FF"></port>
				</connection>
				<connection name="fabric_network_2">
					<port target="20:00:00:02:AC:00:09:00"></port>
					<port target="20:00:00:02:AC:00:09:04"></port>
