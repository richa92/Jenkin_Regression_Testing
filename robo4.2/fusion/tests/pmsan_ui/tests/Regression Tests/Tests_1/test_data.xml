<data>
	<!-- The Environmnet to setup the system -->
	<environment_configuration>
		<env seleniumSpeed="0.1" applianceIP="https://siddvenk.vse.adapps.hp.com" browser="chrome"/>
	</environment_configuration>

	<!-- The User who will log on to the appliance -->
	<users>
		<user name="Administrator" password="hpvse123"/>
	</users>

	<!-- Initial configuration for profile_with_san -->
	<profilewithsanstorage>
		<profile name="Profile_with_SAN" server="Encl1, bay 4" mac="Virtual" serial="Virtual" hardwaretype="BL460c Gen8 1" enclgroup="group_1">
			<!--  Connections for profile -->
			<connection name="PrimaryConnection" type="Fibre Channel" network="direct_network_1" portName="Auto" boot="Not Bootable"></connection>
			<connection name="SecondaryConnection" type="Fibre Channel" network="direct_network_2" portName="Auto" boot="Not Bootable"></connection>
			<!-- Initial volumes for profile -->
			<sanstorage sanvolume="New_non_permanent_volume_1" san="true" volumetype="New Volume" sanlun ="" storagepool="FST_CPG1" capacity="10" provisioning="Full" permanent="False" ostype="Windows 2012 / WS2012 R2">
				<!--  Connection for storage paths -->
				<connection name="direct_network_1"></connection>
				<connection name="direct_network_2"></connection>
			</sanstorage>
		</profile>
	</profilewithsanstorage>

	<!-- Volume configuration after creation -->
	<volumesbeforeprofileedits>
		<volume name="New_non_permanent_volume_1" lun="" storagepool="FST_CPG1" size="10" provisioning="Full" permanent="No" sharing="private">
			<!-- Connections for this volume -->
			<connection name="direct_network_1" type="direct attach" enabled="yes"></connection>
			<connection name="direct_network_2" type="direct attach" enabled="yes"></connection>
		</volume>
	</volumesbeforeprofileedits>

	<!-- Edit 1 configuration for profile_with_san -->
	<profilewithsanstorageedit1>
		<editprofile name="Profile_with_SAN">
			<sanstorage sanvolume="New_non_permanent_volume_1" san="true" modification="delete"></sanstorage>
		</editprofile>
	</profilewithsanstorageedit1>

	<!-- Volume tables after first edit -->
	<volumesafterprofileedit1/>

	<!-- Volumes page after first edit -->
	<volumesafterprofileedit1_>
		<volume name="New_non_permanent_volume_1" deleted=""></volume>
	</volumesafterprofileedit1_>

	<!-- Edit 2 configuration for profile_with_san -->
	<profilewithsanstorageedit2>
		<editprofile name="Profile_with_SAN">
			<sanstorage sanvolume="New_permanent_volume_1" san="true" modification="add" volumetype="New volume" storagepool="FST_CPG1" sanlun="" capacity="10" provisioning="Thin" permanent="True">
				<!-- Connections for this volume -->
				<connection name="direct_network_1" type="direct attach" enabled="yes"></connection>
				<connection name="direct_network_2" type="direct attach" enabled="yes"></connection>
			</sanstorage>
			<sanstorage sanvolume="New_non_permanent_volume_2" san="true" modification="add" volumetype="New volume" storagepool="FST_CPG1" sanlun="" capacity="10" provisioning="Full" permanent="False">
				<!-- Connections for this volume -->
				<connection name="direct_network_1" type="direct attach" enabled="yes"></connection>
				<connection name="direct_network_2" type="direct attach" enabled="yes"></connection>
			</sanstorage>
		</editprofile>
	</profilewithsanstorageedit2>

	<!-- Volume tables after second edit -->
	<volumesafterprofileedit2>
		<volume name="New_permanent_volume_1" lun="" storagepool="FST_CPG1" size="10" permanent="Yes" sharing="private" provisioning="Thin">
			<connection name="direct_network_1" type="direct attach" enabled="yes"></connection>
			<connection name="direct_network_2" type="direct attach" enabled="yes"></connection>
		</volume>
		<volume name="New_non_permanent_volume_2" lun="" storagepool="FST_CPG1" size="10" permanent="No" sharing="private" provisioning="Full">
			<connection name="direct_network_1" type="direct attach" enabled="yes"></connection>
			<connection name="direct_network_2" type="direct attach" enabled="yes"></connection>
		</volume>
	</volumesafterprofileedit2>

	<!-- Volumes page after second edit -->
	<volumesafterprofileedit2_>
		<volume name="New_permanent_volume_1" remain=""></volume>
		<volume name="New_non_permanent_volume_2" remain=""></volume>
	</volumesafterprofileedit2_>

	<!-- Deleted profile -->
	<deletedprofiles>
		<profile name="Profile_with_SAN" nonpermanentvolume=""></profile>
	</deletedprofiles>

	<!-- Volumes page after deletion -->
	<volumesafterprofiledeletion>
		<volume name="New_permanent_volume_1" remain=""></volume>
		<volume name="New_non_permanent_volume_2" deleted=""></volume>
	</volumesafterprofiledeletion>

</data>