<!-- Description:  This is a sample XML script to set the Lights-Out -->
<!--               device to factory default settings.               -->

<!-- NOTE:  You will need to replace the USER_LOGIN and PASSWORD     -->
<!--        values with values that are appropriate for your         -->
<!--        environment.                                             -->

<!--        Use CPQLOCFG.EXE ver 4.00 or greater with this script    -->

<!--        This script was written for iLO 4 firmware version 1.01  -->
<!--        release.                                                 -->

<!--        See the iLO User Guide for more information on scripting -->
<!--        and the syntax of the RIBCL XML.                         -->

<!--        Firmware support information for this script:            -->
<!--            iLO 4 - All versions.                                -->
<!--            iLO 3 - All versions.                                -->
<!--            iLO 2 - All versions.                                -->

<RIBCL VERSION="2.0">
   <LOGIN USER_LOGIN="Administrator" PASSWORD="hpvse123">
      <RIB_INFO MODE="write">
         <FACTORY_DEFAULTS/>
      </RIB_INFO>
   </LOGIN>
</RIBCL>
