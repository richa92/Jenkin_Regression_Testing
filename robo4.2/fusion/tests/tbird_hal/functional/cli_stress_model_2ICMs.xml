<scxml xmlns="http://www.w3.org/2005/07/scxml" version="1.0" initial="ICMs">

  <parallel id="ICMs">

        <!--
  <parallel id="ICM_1">
	-->

    <state id="A_1_Session" initial="A_1_Not_Started">
      <state id="A_1_Not_Started">
	<transition event="Start_A_1" target="A_1_Started"/>
      </state>
      <state id="A_1_Started" initial="A_1_Main_View">
        <!--
	<transition event="Exit_A_1" target="A_1_Not_Started"/>
	-->
	<state id="A_1_Main_View">
	  <transition event="ConsoleView_A_1" target="A_1_Console_View"/>
	  <transition event="MainCommand_A_1" target="A_1_Main_View"/>
	</state>
	<state id="A_1_Console_View">
	  <transition event="ViewCommand_A_1" target="A_1_Console_View"/>
	  <transition event="Kill_B_1" target="A_1_Console_View B_1_Console_View" cond="In('B_1_Connection')"/>
 	  <transition event="Connect_ICM_A_1" target="A_1_Connection" cond="!In('B_1_Connection')"/>
	  <transition event="Connect_A_1_Force" target="A_1_Connection" cond="!In('B_1_Connection')" />
	  <transition event="Connect_A_1_Force" target="A_1_Connection B_1_Console_View" cond="In('B_1_Connection')" />
	  <transition event="Quit_Console_View_A_1" target="A_1_Main_View"/>
	</state>
      </state>
      <state id="A_1_Connection">
	<transition event="Quit_Connection_A_1" target="A_1_Console_View"/>
      </state>
    </state>

    <state id="B_1_Session" initial="B_1_Not_Started">
      <state id="B_1_Not_Started">
	<transition event="Start_B_1" target="B_1_Started"/>
      </state>
      <state id="B_1_Started" initial="B_1_Main_View">
        <!--
	<transition event="Exit_B_1" target="B_1_Not_Started"/>
	-->
	<state id="B_1_Main_View">
	  <transition event="ConsoleView_B_1" target="B_1_Console_View"/>
	  <transition event="MainCommand_B_1" target="B_1_Main_View"/>
	</state>
	<state id="B_1_Console_View">
	  <transition event="ViewCommand_B_1" target="B_1_Console_View"/>
	  <transition event="Kill_A_1" target="B_1_Console_View A_1_Console_View" cond="In('A_1_Connection')"/>
	  <transition event="Connect_ICM_B_1" target="B_1_Connection" cond="!In('A_1_Connection')"/>
	  <transition event="Connect_B_1_Force" target="B_1_Connection" cond="!In('A_1_Connection')"/>
	  <transition event="Connect_B_1_Force" target="B_1_Connection A_1_Console_View" cond="In('A_1_Connection')"/>
	  <transition event="Quit_Console_View_B_1" target="B_1_Main_View"/>
	</state>
      </state>
      <state id="B_1_Connection">
	<transition event="Quit_Connection_B_1" target="B_1_Console_View"/>
      </state>
    </state>

        <!--
  </parallel>

  <parallel id="ICM_2">
    -->
    
    <state id="A_2_Session" initial="A_2_Not_Started">
      <state id="A_2_Not_Started">
	<transition event="Start_A_2" target="A_2_Started"/>
      </state>
      <state id="A_2_Started" initial="A_2_Main_View">
        <!--
	<transition event="Exit_A_2" target="A_2_Not_Started"/>
	-->
	<state id="A_2_Main_View">
	  <transition event="ConsoleView_A_2" target="A_2_Console_View"/>
	  <transition event="MainCommand_A_2" target="A_2_Main_View"/>
	</state>
	<state id="A_2_Console_View">
	  <transition event="ViewCommand_A_2" target="A_2_Console_View"/>
	  <transition event="Kill_B_2" target="A_2_Console_View B_2_Console_View" cond="In('B_2_Connection')"/>
 	  <transition event="Connect_ICM_A_2" target="A_2_Connection" cond="!In('B_2_Connection')"/>
	  <transition event="Connect_A_2_Force" target="A_2_Connection" cond="!In('B_2_Connection')" />
	  <transition event="Connect_A_2_Force" target="A_2_Connection B_2_Console_View" cond="In('B_2_Connection')" />
	  <transition event="Quit_Console_View_A_2" target="A_2_Main_View"/>
	</state>
      </state>
      <state id="A_2_Connection">
	<transition event="Quit_Connection_A_2" target="A_2_Console_View"/>
      </state>
    </state>

    <state id="B_2_Session" initial="B_2_Not_Started">
      <state id="B_2_Not_Started">
	<transition event="Start_B_2" target="B_2_Started"/>
      </state>
      <state id="B_2_Started" initial="B_2_Main_View">
        <!--
	<transition event="Exit_B_2" target="B_2_Not_Started"/>
	-->
	<state id="B_2_Main_View">
	  <transition event="ConsoleView_B_2" target="B_2_Console_View"/>
	  <transition event="MainCommand_B_2" target="B_2_Main_View"/>
	</state>
	<state id="B_2_Console_View">
	  <transition event="ViewCommand_B_2" target="B_2_Console_View"/>
	  <transition event="Kill_A_2" target="B_2_Console_View A_2_Console_View" cond="In('A_2_Connection')"/>
	  <transition event="Connect_ICM_B_2" target="B_2_Connection" cond="!In('A_2_Connection')"/>
	  <transition event="Connect_B_2_Force" target="B_2_Connection" cond="!In('A_2_Connection')"/>
	  <transition event="Connect_B_2_Force" target="B_2_Connection A_2_Console_View" cond="In('A_2_Connection')"/>
	  <transition event="Quit_Console_View_B_2" target="B_2_Main_View"/>
	</state>
      </state>
      <state id="B_2_Connection">
	<transition event="Quit_Connection_B_2" target="B_2_Console_View"/>
      </state>
    </state>

        <!--
  </parallel>
  -->

  </parallel>
</scxml>
