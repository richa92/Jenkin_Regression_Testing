#!/robo4.2/4.2/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pycodestyle','console_scripts','pycodestyle'
__requires__ = 'pycodestyle'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pycodestyle', 'console_scripts', 'pycodestyle')()
    )
